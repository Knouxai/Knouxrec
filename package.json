{
  "name": "knoux-rec",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Luxury AI-Powered Screen Recorder with Glassmorphism UI",
  "keywords": [
    "screen-recorder",
    "ai",
    "glassmorphism",
    "offline",
    "typescript",
    "react",
    "luxury-ui",
    "tensorflow"
  ],
  "author": "KNOUX Team",
  "license": "MIT",
  "homepage": "https://github.com/Knouxai/Knouxrec",
  "repository": {
    "type": "git",
    "url": "https://github.com/Knouxai/Knouxrec.git"
  },
  "bugs": {
    "url": "https://github.com/Knouxai/Knouxrec/issues"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npx vite-bundle-analyzer dist",
    "build:prod": "npm run type-check && vite build --mode production",
    "build:dev": "vite build --mode development",
    "serve": "npm run build && npm run preview",
    "deploy": "npm run build:prod && gh-pages -d dist"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-backend-webgl": "^4.15.0",
    "@tensorflow/tfjs-converter": "^4.22.0",
    "fabric": "^6.7.0",
    "fast-xml-parser": "^5.2.5",
    "fft-js": "^0.0.12",
    "jimp": "^1.6.0",
    "lamejs": "^1.2.1",
    "ml-matrix": "^6.12.1",
    "opus-recorder": "^8.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recordrtc": "^5.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "prettier": "^3.1.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 90",
    "Firefox >= 88",
    "Safari >= 14",
    "Edge >= 90"
  ]
}
